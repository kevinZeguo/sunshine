<div class="modal fade cms-modal" id="fileOpenModal" role="dialog" aria-hidden="true"
     style="width: 600px;height: 700px">
    <form class="form-horizontal" id="deviceForm" role="form">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
                <span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        </div>
        <div class="modal-body" id="addDeviceBody">
            <img src="" id="fileImg" style="width: 100%;height: 100%">
        </div>
    </form>
</div>



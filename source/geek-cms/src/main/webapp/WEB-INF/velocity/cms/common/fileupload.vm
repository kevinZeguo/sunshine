<form id="fileUploadForm" class="hidden" id="fileData-form" enctype="multipart/form-data" method="post">
    <input id="file" name="file" type="file"/>

    <div id="fileUploadProgress">
    ##        <div class="progress">
    ##            <span class="bar"></span><span class="percent">0%</span>
    ##        </div>
        <!-- 显示已上传的文件名 -->
        <div class="files"></div>
        <!-- 显示已上传的图片-->
        <div class="showimg"></div>
    </div>
    <input type="submit" id="uploadBtn" name="uploadBtn"/>
</form>

<form id="imgUploadForm" class="hidden" id="imgData-form" enctype="multipart/form-data" method="post">
    <input id="img" name="file" type="file"/>
    <input type="submit" id="uploadImg" name="uploadImg"/>
</form>


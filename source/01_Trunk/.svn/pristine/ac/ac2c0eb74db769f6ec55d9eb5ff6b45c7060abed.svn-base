/* slider */
.slider {
    width: 448px;
    height: 263px;
    position: relative;
    overflow: hidden;
}

.conbox {
    position: absolute; /*必要元素*/
}

.switcher {
    position: absolute;
    bottom: 10px;
    right: 10px;
    float: right;
    z-index: 99;
}

.switcher a {
    background: #fff;
    border: 1px solid #D00000;
    cursor: pointer;
    float: left;
    font-family: arial;
    height: 18px;
    line-height: 18px;
    width: 18px;
    margin: 4px;
    text-align: center;
    color: #D00000;
}

.switcher a.cur, .switcher a:hover {
    background: #FF0000;
    border: 1px solid #D00000;
    height: 24px;
    line-height: 24px;
    width: 24px;
    margin: 0 2px;
    color: #fff;
    font-weight: 800;
}

.pc .ylTop {

#set($layout = "layout_sunshine_fra.vm")
<style>
    .help-block {
        font-size: 11px !important;
        color: red;
        width: 150px;
        float: right;
    }
</style>
<div class="hcr1">
    <div class="r01">
        <div class="r01l">Reservation</div>
        <div class="r01r"><a href="http://www.soluxehotelniamey.com/fra/">Acceuil</a> &gt; <a href="/fra.html">Réservation
        </a> &gt;
            L’information remplir commande
        </div>
    </div>
    <form id="orderForm">
        <div class="oi bgColor" style="background-color: gainsboro;width: 100%">Description de la chambre</div>
        <div class="" id="rinfo" style="border: 1px solid slategray">
            <table width="100%" border="0" cellspacing="0" cellpadding="0" style="margin: 20px 10px 10px 10px;">
                <tbody>
                <tr>
                    <td>
                        <div></div>
                    </td>
                    <td>
                        <div>&nbsp;</div>
                    </td>
                <tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Chambre:
                    </td>
                    <td width="90%">
                        <strong>$!{room.typeName}</strong>
                    </td>
                </tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Type de lit:
                    </td>
                    <td width="85%">
                        <strong>$!{room.badType}</strong>
                    </td>
                </tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Petit dejeuner:
                    </td>
                    <td width="85%">
                        <strong>$!{room.breakfast}</strong>
                    </td>
                </tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Reseau:
                    </td>
                    <td width="85%">
                        <strong>$!{room.network}</strong>
                    </td>
                </tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Niveau:
                    </td>
                    <td width="85%">
                        <strong>$!{room.floor}</strong>
                    </td>
                </tr>
                <tr style="height: 20px">
                    <td width="12%">
                        Resume:
                    </td>
                    <td width="85%">
                        <strong>$!{room.typeNote}</strong>
                    </td>

                </tr>
                </tbody>
            </table>
        </div>
        <div class="oi bgColor" id="brrp"
             style="background-color: gainsboro;width: 100%;margin-top: 5px;margin-left: 1px">$!{room.typeName}</div>
        <div style="display:none;">
            <input type="hidden" id="typeId" name="typeId" value="$!{room.typeId}">
            <input type="hidden" id="startDate" name="startDate" value="$!{startDate}">
            <input type="hidden" id="endDate" name="endDate" value="$!{endDate}">
            <input type="hidden" id="numRetainRooms" name="numRetainRooms" value="$!{room.numRetainRooms}">
            <input id="env" name="env" type="hidden" value="fra">
        </div>
        <div class="" style="border: 1px solid slategray">
            <table width="100%" border="0" cellspacing="0" cellpadding="0" style="margin-left: 10px">
                <tbody>
                <tr>
                    <td style="width:12%; height:30px;">Nombre de chambre:</td>
                    #if($!{countList.size()}  > 0 && $!{room.perStatus} ==1 )
                        <td><select name="orderNumRooms" style="width: 60px" id="orderNumRooms">
                            #foreach($!{rCount} in $!{countList} )
                                <option value="$!{rCount}">$!{rCount}</option>
                            #end
                        </select>
                        </td>
                    #else
                        <td>
                            Pardon, les chambres ont l’âge de réservation!
                        </td>
                    #end

                </tr>
                </tbody>
            </table>
            <table width="100%" border="0" cellspacing="0" cellpadding="0" style="margin-left: 10px">
                <tbody id="inCountHtml">
                <tr>
                    <td style="width:12%; height:30px;">Nombre de client:</td>
                    #if($!{countList.size()}  > 0)
                        <td><select name="inCount" style="width: 60px" id="inCount" class="inCount">
                            <option value="1" selected>1</option>
                            <option value="2">2</option>
                        </select>
                        </td>
                    #else
                        <td>
                            Pardon, les chambres ont l’âge de réservation!
                        </td>
                    #end
                </tr>
                </tbody>
            </table>

            <table width="100%" border="0" cellspacing="0" cellpadding="0" style="margin-left: 10px">
                <tbody>
                <tr>
                    <td style="width:12%; height:30px;">Date d’arrivee:</td>
                    <td>$!{fmtStartDate}
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Date de depart:</td>
                    <td>$!{fmtEndDate}
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Taux au total (xof):</td>

                    <td>
                        <input type="hidden" id="sumSalePrice" name="sumSalePrice" value="$!{salePrice1}">
                        <input type="hidden" id="salePirce" name="salePrice" value="$!{room.salePrice}">
                        <input type="hidden" id="salePirce1" name="salePrice1" value="$!{salePrice1}">
                        <input type="hidden" id="salePirce2" name="salePrice2" value="$!{salePrice2}">
                        <span class="price popup" id="totalPrice"> $!{salePrice1}</span></td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="oi bgColor" id="brrp"
             style="background-color: gainsboro;width: 100%;margin-top: 5px;margin-left: 1px">Informations du client
        </div>
        <div class="" style="border: 1px solid slategray">
            <table width="100%" border="0" cellspacing="0" cellpadding="0" style="margin-left: 10px">
                <tbody>
                <tr>
                    <td style="width:12%; height:30px;">Nom du client:<span class="color1">*</span></td>
                    <td class="form-group1">
                        <div class="names form-control1" id="names">
                            <input id="username" name="username" placeholder="veuillez saisir votre nom"
                                   type="text"
                                   class="text color3"> <span
                                class="color3">d’utiliser</span>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Compagnie:</td>
                    <td class="form-group1">
                        <div class="names form-control1" id="names">
                            <input id="company" name="company" placeholder="veuillez saisir votre Compagnie" type="text"
                                   class="text color3">
                        </div>
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Email<span class="color1">*</span></td>
                    <td class="form-group1">
                        <div class="form-control1">
                            <input placeholder="veuillez saisir votre email" id="email" name="email" type="text"
                                   class="text color3">
                        </div>
                        <div>
                            <span class="text ">D’utiliser des ministères, un contrat de vente par courrier électronique avec vous</span>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Numero de telephone:<span class="color1">*</span></td>
                    <td class="form-group1">
                        <div class="form-control1">
                            <input id="telephone" name="telephone" placeholder="veuillez saisir votre Téléphone"
                                   type="text"
                                   class="text color3">
                        </div>
                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Temps limite d’arrivee:</td>
                    <td>
                        <div>

                            <select style="width: 160px;" id="lateArriTime" name="lateArriTime">
                                <option value="12:00 avant">12:00 avant</option>
                                <option value="13:00">13:00</option>
                                <option value="14:00">14:00</option>
                                <option value="15:00">15:00</option>
                                <option value="16:00">16:00</option>
                                <option value="17:00">17:00</option>
                                <option value="18:00" selected="selected">18:00</option>
                                <option value="19:00">19:00</option>
                                <option value="20:00">20:00</option>
                                <option value="21:00">21:00</option>
                                <option value="22:00">22:00</option>
                                <option value="23:00">23:00</option>
                                <option value="23:59">23:59</option>
                                <option value="Le lendemain1:00">Le lendemain1:00</option>
                                <option value="Le lendemain2:00">Le lendemain2:00</option>
                                <option value="Le lendemain3:00">Le lendemain3:00</option>
                                <option value="Le lendemain4:00">Le lendemain4:00</option>
                                <option value="Le lendemain5:00">Le lendemain5:00</option>
                            </select>
                        </div>
                        <div>
                            <span class="color3">(Prière de s’inscrire en ligne à plus tard dans le temps avant camions chargés de vivres de première nécessité)</span>
                        ##                            ，有<span id="teshu" class="teshu">特殊需求</span></span>
                        </div>

                    </td>
                </tr>
                <tr>
                    <td style="width:12%; height:30px;">Requette speciale:</td>
                    <td class="form-group1">
                        <div class="form-control1">
                            <textarea placeholder="Dans la demande" id="comment" name="comment" cols="70"
                                      rows="4"></textarea>
                        </div>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="oii" style="margin-top: 20px;">
            <input type="hidden" id="needCcv" name="needCcv" value="0">
            <input type="hidden" id="canSubmit" name="canSubmit" value="1">
            <input id="subOrder" type="submit" value="Confirmer" class="submit-btn">
        </div>
    </form>
</div>

<script src="/plug-in/public/hotel/order_fra.js?_dc=$!{staticVersion}"></script>
